<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    android:background="#FEFBE7"
    tools:context=".Fragment5">

    <Button
        android:id="@+id/btn_S_Informaiton"
        android:layout_width="280dp"
        android:layout_height="60dp"
        android:layout_marginTop="104dp"
        android:background="@drawable/btn_circle"
        android:text="내 정보"
        android:textColor="@color/white"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_S_Register"
        android:layout_width="280dp"
        android:layout_height="60dp"
        android:layout_marginTop="50dp"
        android:background="@drawable/btn_circle2"
        android:text="화분 등록"
        android:textColor="@color/white"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/btn_S_Informaiton"
        app:layout_constraintStart_toStartOf="@+id/btn_S_Informaiton"
        app:layout_constraintTop_toBottomOf="@+id/btn_S_Informaiton" />

    <Button
        android:id="@+id/btn_S_Logout"
        android:layout_width="280dp"
        android:layout_height="60dp"
        android:layout_marginTop="50dp"
        android:background="@drawable/btn_circle5"
        android:text="로그아웃"
        android:textColor="@color/white"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/btn_S_Register"
        app:layout_constraintStart_toStartOf="@+id/btn_S_Register"
        app:layout_constraintTop_toBottomOf="@+id/btn_S_Register" />
</androidx.constraintlayout.widget.ConstraintLayout>